<odoo>

    <record model="ir.ui.view" id="view_users_form">
        <field name="name">res.users.form (in odoo_app)</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='button_box']">
                <button name="generate_login_qr" type="object" icon="fa-qrcode"/>
            </xpath>
        </field>
    </record>



</odoo>
